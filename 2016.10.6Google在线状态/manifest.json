{
    "manifest_version": 2,
    "name": "Google在线状态",
    "version": "0.1",
    "description": "监视Google是否在线",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "icon19.png",
            "38": "icon38.png"
        }
    },
    "background": {
        "scripts": [
            "status.js"
        ]
    },
    "permissions": [
        "https://www.google.com/"
    ]
}
